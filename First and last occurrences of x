class GFG
{
    ArrayList<Integer> find(int arr[], int n, int x)
    {
        ArrayList<Integer> al = new  ArrayList<Integer>();
      int first = -1;
      int last = -1;
      
      for(int i=0;i<n;i++){
          if(arr[i] == x){
              first = i;
              break;
          }
      }
      for(int i=0;i<n;i++){
          if(arr[i] == x){
              last = i;
          }
      }
       al.add(first);
       al.add(last);
       return al;
    }
}
